# Production Configuration for Live Coin Watch API Fetcher

# Live Coin Watch API Configuration
LCW_API_KEY=your_production_api_key_here
LCW_BASE_URL=https://api.livecoinwatch.com

# InfluxDB Configuration
INFLUXDB_URL=https://your-influxdb-cloud-url.com
INFLUXDB_TOKEN=your_production_influxdb_token
INFLUXDB_ORG=your_production_org
INFLUXDB_BUCKET=crypto_production

# Application Configuration
LOG_LEVEL=INFO
FETCH_INTERVAL_MINUTES=10
MAX_COINS_PER_FETCH=50
TRACKED_COINS=BTC,ETH,BNB,XRP,ADA,GLQ,MATIC,DOT,LINK,UNI,LTC

# Scheduling Configuration
ENABLE_SCHEDULER=true
SCHEDULER_TIMEZONE=UTC
REQUESTS_PER_MINUTE=30

# Database Configuration
DATABASE_NAME=crypto_production
