        {
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-lcw"
          },
          "query": "// Portfolio Value Calculation (BTC, ETH, GLQ)\nbtc = from(bucket: \"crypto_data\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"coins\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\n  |> filter(fn: (r) => r[\"code\"] == \"BTC\")\n  |> last()\n  |> map(fn: (r) => ({r with _value: r._value * 0.5}))\n\neth = from(bucket: \"crypto_data\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"coins\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\n  |> filter(fn: (r) => r[\"code\"] == \"ETH\")\n  |> last()\n  |> map(fn: (r) => ({r with _value: r._value * 2.0}))\n\nglq = from(bucket: \"crypto_data\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"coins\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\n  |> filter(fn: (r) => r[\"code\"] == \"GLQ\")\n  |> last()\n  |> map(fn: (r) => ({r with _value: r._value * 10000.0}))\n\nunion(tables: [btc, eth, glq])\n  |> sum()",
          "refId": "A"
        }
      ],
      "title": "Total Portfolio Value",
      "type": "stat"
    },
