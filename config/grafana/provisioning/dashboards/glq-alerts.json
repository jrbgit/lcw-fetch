      "pluginVersion": "9.5.2",
      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-lcw"
          },
          "query": "// Price alert levels based on 24h change and volatility\nchange_24h = from(bucket: \"crypto_data\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"cryptocurrency_data\")\n  |> filter(fn: (r) => r[\"_field\"] == \"delta_24h\")\n  |> filter(fn: (r) => r[\"code\"] == \"GLQ\")\n  |> last()\n\nchange_1h = from(bucket: \"crypto_data\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"cryptocurrency_data\")\n  |> filter(fn: (r) => r[\"_field\"] == \"delta_1h\")\n  |> filter(fn: (r) => r[\"code\"] == \"GLQ\")\n  |> last()\n\nunion(tables: [change_24h, change_1h])\n  |> pivot(rowKey: [\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({\n    _time: r._time,\n    _value: if math.abs(x: r.delta_24h) > 20.0 or math.abs(x: r.delta_1h) > 10.0 then 2.0 else if math.abs(x: r.delta_24h) > 10.0 or math.abs(x: r.delta_1h) > 5.0 then 1.0 else 0.0\n  }))\n  |> yield(name: \"price_alert_level\")",
          "refId": "A"
        }
      ],
      "title": "Price Alert Status",
      "type": "stat"
    },
