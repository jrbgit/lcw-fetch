[run]
source = src/
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */site-packages/*
    */venv/*
    */build/*
    */dist/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\(Protocol\):

show_missing = True
precision = 2
fail_under = 80

[html]
directory = htmlcov
title = LCW API Fetcher Coverage Report

[xml]
output = coverage.xml
